/************ ELECTRIC GO-KART MAIN ************/
#include <stdio.h>
#include "platform.h"
#include "xparameters.h"
#include "xil_printf.h"
#include "xscugic.h"
#include "xil_exception.h"

// User includes
#include "axiinterface.h"
#include "xadc.h"

// User defines
static void XAdcInterruptHandler(XAdcPs *XAdc);

// User structs


// User variables


int main()
{
    init_platform();

    initXAdc();
    initGicXAdc((Xil_ExceptionHandler) XAdcInterruptHandler);

    /************************
     * INSERT CODE HERE
     */

    cleanup_platform();
    return 0;
}

static void XAdcInterruptHandler(XAdcPs *XAdc) {
	getBatteryRaw(u16 *raw);
	getTorqueRaw(u16 *raw);
	getPhaseARaw(u16 *raw);
	getPhaseBRaw(u16 *raw);
}
