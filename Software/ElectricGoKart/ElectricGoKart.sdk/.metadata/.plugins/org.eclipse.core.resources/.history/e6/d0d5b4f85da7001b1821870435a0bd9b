#include "axiinterface.h"


// Functions
// See header file for documentation

void readPosition(u32 *raw_pos) {
	*raw_pos = ENCODER_DRIVER_mReadReg(ENCODER_DRIVER_ADDR, ENCODER_DRIVER_S00_AXI_SLV_REG0_OFFSET);
}

void rawToRads(u32 raw_pos, LONG *pos_rad) {
	*pos_rad = (LONG)(2*PI)/(255) * raw_pos;
}

